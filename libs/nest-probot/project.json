{
  "name": "nest-probot",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nest-probot/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nest-probot",
        "tsConfig": "libs/nest-probot/tsconfig.lib.json",
        "packageJson": "libs/nest-probot/package.json",
        "main": "libs/nest-probot/src/index.ts",
        "assets": ["libs/nest-probot/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "buildTarget": "build",
      "options": {
        "npm": true
      }
    }
  }
}
